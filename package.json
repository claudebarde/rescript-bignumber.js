{
  "name": "rescript-bignumber",
  "version": "0.0.1",
  "scripts": {
    "res:build": "rescript",
    "res:clean": "rescript clean",
    "res:start": "rescript build -w",
    "res:test": "rescript && retest lib/js/test/**/*.bs.js",
    "res:watch_test": "nodemon --ext res --exec \"rescript clean -with-deps & rescript build & npm run res:test\""
  },
  "keywords": [
    "rescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bignumber.js": "^9.1.0",
    "rescript": "*"
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "rescript-test": "^4.0.0"
  }
}
